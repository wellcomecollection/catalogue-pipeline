      "@type": "madsrdf:Source",
      "madsrdf:citationNote": {
        "@language": "en",
        "@value": "v. 48, no. 2, Feb., 1999 (Augmenting loop tiling with data alignment for improved cache performance. Loop blocking (tiling) is a well-known compiler optimization that helps improve cache performance by dividing the loop iteration space into smaller blocks (tiles))"
      },
      "madsrdf:citationSource": "IEEE transactions on Computers:",
      "madsrdf:citationStatus": "found"
    },
