      "@id": "_:n5c7514adfaab45d5bc99c13a2faadfc4b5",
      "@type": "madsrdf:Source",
      "madsrdf:citationNote": {
        "@language": "en",
        "@value": "CIP galley (Loop tiling, as one of the most important compiler optimizations, is beneficial for both parallel machines and uniprocessors with a memory hierarchy)"
      },
      "madsrdf:citationSource": "Work cat.: 00057639: Xue, Jingling. Loop tiling for parallelism, c2000:",
      "madsrdf:citationStatus": "found"
    },
    {
